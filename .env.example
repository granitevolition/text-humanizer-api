# OpenAI API Keys
# You can use a single key
OPENAI_API_KEY=your-api-key-here

# Or multiple keys for rotation
OPENAI_API_KEY_1=your-first-api-key
OPENAI_API_KEY_2=your-second-api-key
OPENAI_API_KEY_3=your-third-api-key

# Server configuration
PORT=8000
HOST=0.0.0.0

# Model settings
DEFAULT_MODEL=ft:gpt-3.5-turbo-0125:personal::9hpCfvVt
SELECTION_MODEL=gpt-4o-mini
CLEANING_MODEL=gpt-4o

# Processing settings
TEMPERATURE=1.24
FREQUENCY_PENALTY=0.2
PRESENCE_PENALTY=0.2
SELECT_BEST_SENTENCE=True

# Rate limiting
MAX_REQUESTS_PER_MINUTE=900

# Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO